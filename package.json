{
  "name": "mudae_tools",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "type": "module",
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "electron-build": "electron-builder",
    "package": "electron-builder --dir",
    "make": "electron-builder --win --mac --linux",
    "postinstall": "electron-builder install-app-deps",
    "test": "cypress run",
    "test:ci": "start-server-and-test 'yarn start' http://127.0.0.1:3000 'CYPRESS_ENV=true yarn test'",
    "test:ui": "cypress open --e2e --browser chrome",
    "coverage": "nyc report --reporter=text-summary",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:clear": "rimraf node_modules/.cache/eslint-loader",
    "api:codegen": "SWAGGER_PATH=/swagger TEMPLATES_PATH=./src/API/templates node src/API/swagger-api-to-typescript-code-generator/index.js",
    "tsc": "tsc",
    "tsc:ci": "tsc --noEmit"
  },
  "build": {
    "appId": "com.votre.app",
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": "Use yarn instead"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^7.1.1",
    "@reduxjs/toolkit": "^2.2.3",
    "@tinymce/tinymce-react": "^5.0.1",
    "@types/react": "^18.2.75",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.24",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "electron-is-dev": "^3.0.1",
    "emoji-picker-react": "^4.9.2",
    "formik": "^2.4.5",
    "formik-mui": "^5.0.0-alpha.0",
    "framer-motion": "^11.0.25",
    "fuzzysort": "^2.0.4",
    "http-server": "^14.1.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-activity-calendar": "^2.2.8",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-gesture-responder": "^2.1.0",
    "react-grid-dnd-18": "^1.0.1",
    "react-grid-layout": "^1.4.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "react-toastify": "^10.0.5",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.5",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.4.4",
    "wait-on": "^8.0.1",
    "web-vitals": "^3.5.2",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "xlsx": "^0.18.5",
    "yarn-upgrade-all": "^0.7.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.34",
    "@cypress/instrument-cra": "^1.4.0",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@tiptap/pm": "^2.3.0",
    "@types/cypress__code-coverage": "^3.10.2",
    "@types/node": "^20.12.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "cypress": "^13.7.2",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-modules-newlines": "^0.0.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "istanbul-lib-coverage": "^3.2.2",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.2"
  },
  "nyc": {
    "exclude": [
      "src/API/__generated__"
    ],
    "excludeAfterRemap": true
  }
}
